on rightclick holding a {@menuitem}:
	if {{@arenaid}.iniciou} is true:
		if {{@arenaid}.votacao} is false:
			if {ingame.%player%} is true:
				loop {{@arenaid}.plot.%player%::*}:
					if location of player is within {{@arenaid}.plot.%loop-value-1%.a} to {{@arenaid}.plot.%loop-value-1%.b}:
						send "{@vaparasuaplot}" to player
					else:
						open chest with 1 row named "{@menuitemname}" to player
						wait 5 tick
						format slot 0 of player with a tnt named "{@explodirtudo}" with lore "{@apagartudo}" to close then run [make player execute command "bb{@arenaid} resetmyplot"]
						format slot 1 of player with a end portal named "{@efeitos}" with lore "{@efeitos2}" to run [make player execute command "bbefeitos{@arenaid}"]

command /bbefeitos{@arenaid} <text=menu>:
	executable by: Players
	trigger:
		if arg 1 is "menu":
			if {{@arenaid}.iniciou} is true:
				if {{@arenaid}.votacao} is false:
					if {ingame.%player%} is true:
						loop {{@arenaid}.plot.%player%::*}:
							if location of player is within {{@arenaid}.plot.%loop-value-1%.a} to {{@arenaid}.plot.%loop-value-1%.b}:
								send "{@vaparasuaplot}" to player
							else:
								open chest with 1 row named "{@efeitos}" to player
								wait 5 tick
								format slot 0 of player with a mob spawner named "{@efeitomobspawn}" with lore "{@efeitomobspawnlore}" to close then run [make player execute command "bbefeitos{@arenaid} 1"]
								format slot 1 of player with a portal named "{@efeitoendersignal}" with lore "{@efeitoendersignallore}" to close then run [make player execute command "bbefeitos{@arenaid} 2"]
								format slot 2 of player with a blaze powder named "{@efeitosmoke}" with lore "{@efeitosmokelore}" to close then run [make player execute command "bbefeitos{@arenaid} 4"]
							stop
						stop
					stop
				stop
			stop
		if arg 1 is "1":
			add location of player to {{@arenaid}.effects.mob_spawner_flames::*}
			stop
		if arg 1 is "2":
			add location of player to {{@arenaid}.effects.ender_signal::*}
			stop
		if arg 1 is "4":
			add location of player to {{@arenaid}.effects.smoke::*}
			stop

