command /bbapurarvotos{@arenaid}:
	executable by: Console
	trigger:
		loop {{@arenaid}.jogadores::*}:
			if {{@arenaid}.votos.%loop-value-1%} is more than {{@arenaid}.score-1}:
				set {{@arenaid}.score-3} to {{@arenaid}.score-2}
				set {{@arenaid}.score-3own} to {{@arenaid}.score-2own}
				set {{@arenaid}.score-2} to {{@arenaid}.score-1}
				set {{@arenaid}.score-2own} to {{@arenaid}.score-1own}
				set {{@arenaid}.score-1} to {{@arenaid}.votos.%loop-value-1%}
				set {{@arenaid}.score-1own} to loop-value-1
			else:
				if {{@arenaid}.votos.%loop-value-1%} is more than {{@arenaid}.score-2}:
					set {{@arenaid}.score-3} to {{@arenaid}.score-2}
					set {{@arenaid}.score-3own} to {{@arenaid}.score-2own}
					set {{@arenaid}.score-2} to {{@arenaid}.votos.%loop-value-1%}
					set {{@arenaid}.score-2own} to loop-value-1
				else:
					if {{@arenaid}.votos.%loop-value-1%} is more than {{@arenaid}.score-3}:
						set {{@arenaid}.score-3} to {{@arenaid}.votos.%loop-value-1%}
						set {{@arenaid}.score-3own} to loop-value-1
		execute console command "/bbvencedores{@arenaid}"

command /bbvencedores{@arenaid}:
	executable by: Console
	trigger:
		if {{@arenaid}.iniciou} is true:
			set {{@arenaid}.votacao} to false
			set {{@arenaid}.vencedores} to true
			loop {{@arenaid}.plot.%{{@arenaid}.score-1own}%::*}:
