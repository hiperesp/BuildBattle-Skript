				if {ingame.%player%} is true:
					loop {{@arenaid}.plot.%player%::*}:
						if location of event-block is within {{@arenaid}.plot.%loop-value-1%.a} to {{@arenaid}.plot.%loop-value-1%.b}:
							cancel event
			if {{@arenaid}.votacao} is true:
				cancel event



on break:
	if {ingame.%player%} is true:
