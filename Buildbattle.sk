command /bb{@arenaid} <text=help>:
	executable by: Players
	trigger:
		if arg 1 is "join":
			if {ingame.%player%} is not true or false:
				send "{@ocorreuerro}" to player
				send "{@tentando}" to player
				set {ingame.%player%} to false
				wait 1 second
				make player execute "/bb{@arenaid} join"			
			if {ingame.%player%} is true:
				send "{@vocejaestanapartida}" to player
			if {ingame.%player%} is false:
				if {{@arenaid}.iniciou} is true:
					send "{@aguardeapartidaacabar}" to player
				else:
					if size of {{@arenaid}.plots.livres::*} is more than 0:
						send "{@entrandonapartida}" to player
						create id based score "" in sidebar of player to 0 with id "12.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "11.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "10.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "9.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "8.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "7.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "6.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "5.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "4.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "3.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "2.%player%.{@arenaid}"
						create id based score "" in sidebar of player to 0 with id "1.%player%.{@arenaid}"
						set {ingame.%player%} to true
						set {arena.%player%} to {@arenaid}
						set {{@arenaid}.tempo} to {@mintempostart}
						add player to {{@arenaid}.jogadores::*}
						teleport player to {{@arenaid}.lobbywait}
						loop {{@arenaid}.plots.livres::*}:
							add loop-value-1 to {{@arenaid}.plot.%player%::*}
							remove loop-value-1 from {{@arenaid}.plots.livres::*}
							stop
					else:
						send "{@partidalotada}" to player
			stop
		if arg 1 is "resetmyplot":
			if {{@arenaid}.iniciou} is true:
				if {ingame.%player%} is true:
					loop {{@arenaid}.plot.%player%::*}:
						loop blocks within {{@arenaid}.plot.%loop-value-1%.a} to {{@arenaid}.plot.%loop-value-1%.b}:
							set loop-block to air
					stop
				stop
			stop
		if arg 1 is "leave":
			set {ingame.%player%} to false
			remove player from {%{arena.%player%}%.jogadores::*}
			send "{@vocesaiudapartida}" to player
			if {%{arena.%player%}%.iniciou} is false:
				loop {%{arena.%player%}%.plot.%player%::*}:
					add loop-value-1 to {%{arena.%player%}%.plots.livres::*}
			loop {%{arena.%player%}%.plots::*}:
				remove loop-value-1 from {%{arena.%player%}%.plot.%player%::*}
			teleport player to {{@arenaid}.spawn}
			clear inventory of player
			display board named "&6&lBuild Battle" to player
			move display of player to sidebar
			delete {arena.%player%}
			stop
		if arg 1 is "help":
			send "{@helpjoin}" to player
			send "{@helpleave}" to player

command /bbadmin{@arenaid} <text=help> [<text>] [<text>]:
	executable by: Players
	trigger:
		if player has permission "{@permissionadmin}":
			if arg 1 is set:
				if arg 1 is "setplot":
					if arg 2 is set:
						if arg 3 is set:
							set {{@arenaid}.plot.%arg 2%.%arg 3%} to player's location
							send "{@ponto} %arg 3% {@daplot} %arg 2% {@setadapara} %player's location%"
							if arg 3 is "a":
								remove arg 2 from {{@arenaid}.plots::*}
								remove arg 2 from {{@arenaid}.plots::*}
								add arg 2 to {{@arenaid}.plots::*}
								stop
							stop
						else:
							send "{@helpplotadd}" to player
							stop
				if arg 1 is "addtema":
					if arg 2 is set:
						add arg 2 to {bb.themes::*}
						send "{@Tema} %arg 2% {@addcomsucesso}" to player
						stop
					stop
				if arg 1 is "removertema":
					if arg 2 is set:
						remove arg 2 from {bb.themes::*}
						send "{@Tema} %arg 2% {@remcomsucesso}" to player
						stop
					stop
				if arg 1 is "temas":
					send "&k{@iniciothemelist}" to player
					send "&c{@iniciothemelist}" to player
					loop {bb.themes::*}:
						send "&a%loop-value-1%" to player
					send "&c{@fimthemelist}" to player
					send "&k{@fimthemelist}" to player
					stop
				if arg 1 is "setspawn":
					set {{@arenaid}.spawn} to player's location
					send "[Re]Spawn {@setadopara} %player's location%"
					stop
				if arg 1 is "setlobbywait":
					set {{@arenaid}.lobbywait} to player's location
					send "LobbyWait {@setadopara} %player's location%"
					stop
				if arg 1 is "playerlist":
					send "%{{@arenaid}.jogadores::*}%" to player
					stop
				if arg 1 is "plotlivres":
					send "%{{@arenaid}.plots.livres::*}%" to player
					stop
				if arg 1 is "plots":
					send "%{{@arenaid}.plots::*}%" to player
					stop
				if arg 1 is "playerplot":
					loop {{@arenaid}.jogadores::*}:
						send "%loop-value-1%: %{{@arenaid}.plot.%loop-value-1%::*}%" to player
					stop
				if arg 1 is "configok":
					execute console command "/bbresetall{@arenaid}"
				if arg 1 is "testplot":
					if arg 2 is set:
						if arg 3 is set:
							teleport player to location at {{@arenaid}.plot.%arg 2%.%arg 3%}
							loop blocks within {{@arenaid}.plot.%arg 2%.%arg 3%} to {{@arenaid}.plot.%arg 2%.%arg 3%}:
								set loop-block to stone
							wait 2 seconds
							loop blocks within {{@arenaid}.plot.%arg 2%.%arg 3%} to {{@arenaid}.plot.%arg 2%.%arg 3%}:
								set loop-block to air
				if arg 1 is "editmode":
					if {{@arenaid}.editmode.%player%} is true:
						set {{@arenaid}.editmode.%player%} to false
					else:
						set {{@arenaid}.editmode.%player%} to true
					send "Edit mode {@setadopara} %{{@arenaid}.editmode.%player%}%" to player
				if arg 1 is "stop":
					execute console command "/bbresetall{@arenaid}"
				if arg 1 is "start":
					execute console command "/bbstartgame{@arenaid}"
				if arg 1 is "help":
					send "{@helpadm0}" to player
					send "{@helpadm1}" to player
					send "{@helpadm2}" to player
					send "{@helpadm15}" to player
					send "{@helpadm3}" to player
					send "{@helpadm4}" to player
					send "{@helpadm5}" to player
					send "{@helpadm14}" to player
				if arg 1 is "help2":
					send "{@helpadm6}" to player
					send "{@helpadm7}" to player
					send "{@helpadm8}" to player
					send "{@helpadm9}" to player
					send "{@helpadm10}" to player
					send "{@helpadm12}" to player
					send "{@helpadm13}" to player
		else:
			send "{@sempermissao}" to player

on command:
	if {ingame.%player%} is true:
		command is not "/bb{@arenaid}" or "/bbadmin{@arenaid}":
			cancel event

command /bbresetall{@arenaid}:
	executable by: Console
	trigger:
		if {{@arenaid}.fix} is equal to 0:
			set {{@arenaid}.fix} to 3
			stop
		set {{@arenaid}.fix} to {{@arenaid}.fix}-1
		set {{@arenaid}.plots.livres} to 4
		set {{@arenaid}.iniciou} to false
		set {{@arenaid}.votacao} to false
		set {{@arenaid}.votacao2} to false
		set {{@arenaid}.iniciando} to false
		set {{@arenaid}.vencedores} to false
		set {{@arenaid}.mob.spawn} to false
		loop {{@arenaid}.jogadores::*}:
			loop {{@arenaid}.votou.%loop-value-1%::*}:
				remove loop-value-2 from {{@arenaid}.votou.%loop-value-1%::*}
			remove loop-value-1 from {{@arenaid}.jogadores::*}
			display board named "&6&lBuild Battle" to loop-value-1
			move display of loop-value-1 to sidebar
			set {ingame.%loop-value-1%} to false
			remove loop-value-1 from {{@arenaid}.jogadores::*}
			clear inventory of loop-value-1
			clear the loop-value-1's inventory
			teleport loop-value-1 to {{@arenaid}.spawn}
			loop {{@arenaid}.plots::*}:
				remove loop-value-2 from {{@arenaid}.plot.%loop-value-1%::*}
				remove loop-value-2 from {{@arenaid}.plot.%loop-value-1%::*}
				remove loop-value-2 from {{@arenaid}.plot.%loop-value-1%::*}
				remove false from {{@arenaid}.plot.%loop-value-1%::*}
				remove false from {{@arenaid}.plot.%loop-value-1%::*}
				remove false from {{@arenaid}.plot.%loop-value-1%::*}
			set {{@arenaid}.votos.%loop-value-1%} to 0
		loop {{@arenaid}.plots::*}:
			remove loop-value-1 from {{@arenaid}.plots.livres::*}
			remove loop-value-1 from {{@arenaid}.plots.livres::*}
			remove loop-value-1 from {{@arenaid}.plots.livres::*}
		loop {{@arenaid}.plots::*}:
			add loop-value-1 to {{@arenaid}.plots.livres::*}
		loop {{@arenaid}.plots::*}:
			loop blocks within {{@arenaid}.plot.%loop-value-1%.a} to {{@arenaid}.plot.%loop-value-1%.b}:
				set loop-block to air
		set {{@arenaid}.plotowner2} to false
		loop {{@arenaid}.votar::*}:
			remove loop-value-1 from {{@arenaid}.votar::*}	
		loop {{@arenaid}.effects.mob_spawner_flames::*}:
			remove loop-value-1 from {{@arenaid}.effects.mob_spawner_flames::*}
		loop {{@arenaid}.effects.ender_signal::*}:
			remove loop-value-1 from {{@arenaid}.effects.ender_signal::*}
		loop {{@arenaid}.effects.smoke::*}:
			remove loop-value-1 from {{@arenaid}.effects.smoke::*}
		set {{@arenaid}.score-1} to 0
		set {{@arenaid}.score-2} to 0
		set {{@arenaid}.score-3} to 0
		delete {{@arenaid}.score-1own}
		delete {{@arenaid}.score-2own}
		delete {{@arenaid}.score-3own}
		set {{@arenaid}.plotowner} to false
		execute console command "/bbkillmobs{@arenaid}"

on join:
	set {ingame.%player%} to false
	remove player from {{@arenaid}.jogadores::*}
	loop {{@arenaid}.plots::*}:
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
	clear inventory of player
	display board named "&6&lBuild Battle" to player
	move display of player to sidebar
	if player is "Banco_Tgames":
		broadcast "&d&l[SERVER] O criador do skript de BuildBattle utilizado neste servidor (&4&lBanco_Tgames&d&L) está aqui!"

on quit:
	set {ingame.%player%} to false
	remove player from {{@arenaid}.jogadores::*}
	loop {{@arenaid}.plots::*}:
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove loop-value-1 from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
		remove false from {{@arenaid}.plot.%player%::*}
	teleport player to {{@arenaid}.spawn}
	clear inventory of player
	display board named "&6&lBuild Battle" to player
	move display of player to sidebar

on rightclick on sign:
	if line 1 is "[BB]" or "{@sign1}":
		if line 2 is "join" or "{@sign2j}":
			if line 4 is "{@arenaid}" or "{@sign4}{@arenaid}":
				make player execute "/bb{@arenaid} join"
				set line 1 to "{@sign1}"
				set line 2 to "{@sign2j}"
				set line 3 to "{@sign3}%size of {{@arenaid}.jogadores::*}%/%size of {{@arenaid}.plots::*}%"
				set line 4 to "{@sign4}{@arenaid}"
		if line 2 is "leave" or "{@sign2l}":
			if line 4 is "{@arenaid}" or "{@sign4}{@arenaid}":
				make player execute "/bb{@arenaid} leave"
				set line 1 to "{@sign1}"
				set line 2 to "{@sign2l}"
				set line 3 to " "
				set line 4 to "{@sign4}{@arenaid}"

on damage:
	cancel event

command /bbstartgame{@arenaid}:
	executable by: Console
	trigger:
		set {{@arenaid}.iniciando} to false
		send "{@partidainiciou}" to {{@arenaid}.jogadores::*}
		clear inventory of {{@arenaid}.jogadores::*}
		set {{@arenaid}.iniciou} to true
		set {{@arenaid}.tempo} to {@tempo}
		set {{@arenaid}bb.theme} to random element of {bb.themes::*}
		loop {{@arenaid}.jogadores::*}:
			set the loop-value-1's gamemode to creative
			loop {{@arenaid}.plot.%loop-value-1%::*}:
				teleport loop-value-1 to {{@arenaid}.plot.%loop-value-2%.a}
		set {{@arenaid}.mob.spawn} to false
		loop {{@arenaid}.jogadores::*}:
			loop {{@arenaid}.votou.%loop-value-1%::*}:
				remove loop-value-2 from {{@arenaid}.votou.%loop-value-1%::*}
		execute console command "bbkillmobs{@arenaid}"

on spawn:
	cancel event

on rightclick holding a spawn egg:
	cancel event

every second:
	if {{@arenaid}.iniciou} is true:
		loop {{@arenaid}.jogadores::*}:
			set the loop-value-1's gamemode to creative
	set {{@arenaid}.2tempo} to {{@arenaid}.tempo}+30
	set {{@arenaid}.ttempo} to rounded {{@arenaid}.2tempo}/60
	set {{@arenaid}.tttempo} to rounded {{@arenaid}.ttempo}*60
	if {{@arenaid}.tempo}-{{@arenaid}.tttempo}+60 is less than 10:
		set {{@arenaid}.segundoscorretos} to "0%{{@arenaid}.tempo}-{{@arenaid}.tttempo}+60%"
