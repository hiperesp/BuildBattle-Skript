command /bbiniciarvotacao{@arenaid}:
	executable by: Console
	trigger:
		if {{@arenaid}.iniciou} is true:
			set {{@arenaid}.votacao} to true
			loop {{@arenaid}.jogadores::*}:
				loop {{@arenaid}.plot.%loop-value-1%::*}:
					add loop-value-2 to {{@arenaid}.votar::*}
			wait 5 seconds
			set {{@arenaid}.votacao2} to true
			execute console command "/bbiniciarvotacaosegundo{@arenaid}"

command /bbiniciarvotacaosegundo{@arenaid}:
	executable by: Console
	trigger:
		if {{@arenaid}.votacao} is true:
			loop size of {{@arenaid}.votar::*} times:
				loop {{@arenaid}.votar::*}:
					loop {{@arenaid}.jogadores::*}:
						loop {{@arenaid}.plot.%loop-value-2%::*}:
							if loop-value-1 is equal to loop-value-3:
								set {{@arenaid}.plotowner2} to {{@arenaid}.plotowner}
								set {{@arenaid}.plotowner} to loop-value-2
					remove loop-value-1 from {{@arenaid}.votar::*}
					teleport {{@arenaid}.jogadores::*} to {{@arenaid}.plot.%loop-value-1%.a}
					set {{@arenaid}.tempo} to {@tempovote}
					loop {@tempovote} times:
						set {{@arenaid}.tempo} to {{@arenaid}.tempo}-1
						wait 1 second
					loop size of {{@arenaid}.jogadores::*} times:
						loop {{@arenaid}.jogadores::*}:
							if loop-value-2 isn't equal to {{@arenaid}.plotowner}:
								if {{@arenaid}.votou.%loop-value-2%::%{{@arenaid}.plotowner}%} is not set:
									add 3 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
									set {{@arenaid}.votou.%loop-value-2%::%{{@arenaid}.plotowner}%} to true
			execute console command "/bbapurarvotos{@arenaid}"
			stop

on drop:
	cancel event

on rightclick holding a {@voteitemgeral}:
	if {ingame.%player%} is true:
		if {arena.%player%} is equal to {@arenaid}:
			if {{@arenaid}.votacao2} is true:
				if player is equal to {{@arenaid}.plotowner}:
					send "{@vocenaopodevotar}" to player
				else:
					if {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} is not set:
						if player is holding a {@voteitem1} named "{@voteitem1name}":
							send "{@vocevotou} {@voteitem1name}" to player
							add 1 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							stop
						if player is holding a {@voteitem2}:
							send "{@vocevotou} {@voteitem2name}" to player
							add 2 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							stop
						if player is holding a {@voteitem3}:
							send "{@vocevotou} {@voteitem3name}" to player
							add 3 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							stop
						if player is holding a {@voteitem4}:
							send "{@vocevotou} {@voteitem4name}" to player
							add 4 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							stop
						if player is holding a {@voteitem5}:
							send "{@vocevotou} {@voteitem5name}" to player
							add 5 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							stop
						if player is holding a {@voteitem6}:
							send "{@vocevotou} {@voteitem6name}" to player
							add 6 to {{@arenaid}.votos.%{{@arenaid}.plotowner}%}
							set {{@arenaid}.votou.%player%::%{{@arenaid}.plotowner}%} to true
							send "{@vocerecebeulegendary} {@voteitem6name}" to {{@arenaid}.plotowner}
